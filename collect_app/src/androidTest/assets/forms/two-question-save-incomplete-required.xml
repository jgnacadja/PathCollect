<?xml version="1.0"?>
<h:html xmlns:h="http://www.w3.org/1999/xhtml" xmlns="http://www.w3.org/2002/xforms">
    <h:head>
        <h:title>Two Question Save Incomplete Required</h:title>
        <model>
            <instance>
                <data id="two_question">
                    <name />
                    <age />
                </data>
            </instance>
            <bind nodeset="name" type="string" />
            <bind nodeset="age" required="true()" saveIncomplete="true()" type="int" />
        </model>
    </h:head>
    <h:body>
        <input ref="/data/name">
            <label>What is your name?</label>
        </input>
        <input ref="/data/age">
            <label>[saveIncomplete] What is your age?</label>
        </input>
    </h:body>
</h:html>
