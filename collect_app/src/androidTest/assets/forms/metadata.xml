<?xml version="1.0"?>
<h:html xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns="http://www.w3.org/2002/xforms">
    <h:head>
        <h:title>Metadata</h:title>
        <model>
            <instance>
                <preload id="preload" version="2017061601">
                    <username />
                    <email />
                    <phonenumber />
                    <deviceID />
                    <subscriberID />
                    <simSerial />
                    <meta>
                        <instanceID />
                    </meta>
                </preload>
            </instance>
            <bind nodeset="/preload/username" readonly="true()" type="string"
                jr:preload="property" jr:preloadParams="username" />
            <bind nodeset="/preload/email" readonly="true()" type="string"
                jr:preload="property" jr:preloadParams="email" />
            <bind nodeset="/preload/phonenumber" readonly="true()" type="string"
                jr:preload="property" jr:preloadParams="phonenumber" />
            <bind nodeset="/preload/deviceID" readonly="true()" type="string"
                jr:preload="property" jr:preloadParams="deviceID" />
            <bind calculate="concat('uuid:', uuid())" nodeset="/preload/meta/instanceID"
                readonly="true()" type="string" />
        </model>
    </h:head>
    <h:body>
        <group appearance="field-list">
            <input ref="/preload/username">
                <label>Username</label>
            </input>
            <input ref="/preload/email">
                <label>Email</label>
            </input>
            <input ref="/preload/phonenumber">
                <label>Phone number</label>
            </input>
            <input ref="/preload/deviceID">
                <label>Device ID</label>
            </input>
            <input ref="/preload/subscriberID">
                <label>Subscriber ID</label>
            </input>
            <input ref="/preload/simSerial">
                <label>SIM serial number</label>
            </input>
        </group>
    </h:body>
</h:html>
